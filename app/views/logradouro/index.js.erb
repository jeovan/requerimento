$('#table_logradouro').html(' <%= escape_javascript(render "logradouro/table_logradouro") %> ')
$('#modal_logradouro').find('input[name="data-input"]').val('<%= @input_id%>')
$('#modal_logradouro').modal('show');

$(document).on('click', '.btn-logradouro',function(){
  var id = `#${ $(this).attr('data-input') }`
  var valor = `${ $(this).attr('data-valor') }`
  $(id).val(valor)
  $('#modal_logradouro').modal('hide')
})