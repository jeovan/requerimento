<div id="modal_<%= tipo%>" class="modal fade">
    <div class="modal-dialog  modal-lg">
        <div class="modal-content">
            <div class="modal-header bg-blue" style="background-color:#286fa5 !important;border-color:#286fa5">
                <button type="button" class="close" data-dismiss="modal">&times;</button>
                <h6 class="modal-title" >BUSCA POR <%= tipo.upcase %></h6>
            </div>

            <div class="modal-body" >
                <form class="col-md-12" id="" action="/<%= tipo %>" data-remote="true" accept-charset="UTF-8" method="get">
                  <div class="row">
                    <div class="form-group col-md-10" style="padding:0">
                      <label for="q_Lote"><%= tipo.upcase %></label>
                      <input type="hidden" name="data-input">
                      <input class="form-control" type="search" name="<%= tipo %>" >
                    </div>

                    <div class="form-group col-md-2 pull-right" style="padding-right: 0px !important">
                      <label style="color: transparent;" for="q_-">-</label>
                      <input type="submit" name="commit" value="Buscar" class="btn btn-primary form-control legitRipple" style="background-color: #286fa5 !important;" data-disable-with="Buscar">
                    </div>

                    </div>
                </form>
               
               <div id="table_<%= tipo%>">
               
               </div>

            </div>

            <div class="modal-footer">
                <button type="button" class="btn btn-link" data-dismiss="modal">FECHAR</button>
            </div>
        </div>
    </div>
</div>