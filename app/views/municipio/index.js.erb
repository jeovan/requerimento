$('#table_municipio').html(' <%= escape_javascript(render "municipio/table_municipio") %> ')
$('#modal_municipio').find('input[name="data-input"]').val('<%= @input_id%>')
$('#modal_municipio').modal('show');

$(document).on('click', '.btn-municipio',function(){
  var id = `#${ $(this).attr('data-input') }`
  var valor = `${ $(this).attr('data-valor') }`
  $(id).val(valor)
  $('#modal_municipio').modal('hide')
})